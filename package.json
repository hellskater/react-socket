{
  "name": "@hellskater/react-socket",
  "version": "0.0.2",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/hellskater"
  },
  "description": "Set of a helpful hooks, which makes interaction with Socket apis easier and more efficient in a React app.",
  "author": "hellskater",
  "license": "MIT",
  "repository": "hellskater/react-socket",
  "sideEffects": false,
  "main": "./index.js",
  "module": "./esm/index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "lint": "eslint ./src --ext ts,tsx --max-warnings 0",
    "lint:fix": "npm run lint --fix",
    "test": "jest src",
    "test:watch": "npm run test -- --watch",
    "build:esm": "tsc",
    "build:cjs": "tsc -p tsconfig-require.json",
    "clean": "rimraf lib",
    "build": "npm run build:esm && npm run build:cjs",
    "postversion": "npm run clean && npm run build",
    "publish": "npm publish --access public"
  },
  "peerDependencies": {
    "react": ">18.2.0",
    "react-dom": ">18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.2.4",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "5.45.1",
    "@typescript-eslint/parser": "5.45.1",
    "babel-eslint": "10.1.0",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "^29.3.1",
    "np": "7.6.2",
    "prettier": "2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "favoriteScripts": [
    "test:watch"
  ]
}
